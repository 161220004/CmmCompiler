int main1()
{
  int n;
  n = read();
  if (n > 0) write(1);
  else if (n < 0) write (-1);
  else write(0);
  return 0;
}

// 2
int fact(int n)
{
  if (n == 1)
  return n;
  else
  return (n * fact(n - 1));
}
int main2()
{
  int m, result;
  m = read();
  if (m > 1)
  result = fact(m);
  else
  result = 1;
  write(result);
  return 0;
}

